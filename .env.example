# Copy to .env.local and fill in values.

# --- Supabase ---
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID=
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=

# Site origin used for auth callbacks when request headers are unavailable.
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Brand resolution for multi-brand auth/customer sync.
# Option A: set NEXT_PUBLIC_BRAND_SLUG per deployed brand domain.
NEXT_PUBLIC_BRAND_SLUG=
# Option B: map incoming hostnames to brand slugs, e.g.:
# BRAND_DOMAIN_MAP=cti.example.com=cti,karen.example.com=karen-miles
BRAND_DOMAIN_MAP=

# --- RudderStack ---
NEXT_PUBLIC_RUDDERSTACK_WRITE_KEY=
NEXT_PUBLIC_RUDDERSTACK_DATA_PLANE_URL=
NEXT_PUBLIC_RUDDERSTACK_DISABLED=false

# Required outside local development: protects POST /api/events
EVENTS_API_KEY=

# --- ClickHouse ---
CLICKHOUSE_URL=
CLICKHOUSE_USER=
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DATABASE=

# Optional: comma-separated hostname allowlist (no scheme/port), supports *.example.com
# Decorates outbound links to these hosts with ?anonymous_id=...
NEXT_PUBLIC_CROSS_DOMAIN_TRACKING_DOMAINS=

# --- Brand config ---
NEXT_PUBLIC_BRAND_NAME=
NEXT_PUBLIC_BRAND_LOGO_URL=
NEXT_PUBLIC_SUPPORT_EMAIL=

# --- Resend SMTP (Supabase Auth email provider) ---
RESEND_SMTP_KEY=
RESEND_FROM_EMAIL=
RESEND_API_KEY=

# --- Booking notifications ---
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=
BOOKING_REMINDER_API_KEY=

# VideoNest
VIDEONEST_API_KEY=your-videonest-api-key
VIDEONEST_CHANNEL_ID=your-channel-id
